{
  "name": "heart-live-monitoring",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && electron-rebuild",
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "npm-run-all -p start:web electron:serve",
    "start:web": "ng serve --port=4200 --configuration=development",
    "ng:build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build --base-href ./ && electron .",
    "electron:build": "ng build -c production && electron-builder",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- --configuration=development",
    "build:prod": "npm run build -- --configuration=production",
    "web:dev": "ng serve --configuration=development",
    "web:prod": "ng serve --configuration=production",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http://localhost:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:dev && npx electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "d3": "^7.9.0",
    "moment": "^2.30.1",
    "npm-run-all": "^4.1.5",
    "rxjs": "~7.8.0",
    "serialport": "^13.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tree": "^0.1.3",
    "tslib": "^2.3.0",
    "wait-on": "^8.0.3",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.3",
    "@angular/cli": "^19.2.3",
    "@angular/compiler-cli": "^19.2.0",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "~5.1.0",
    "@types/serialport": "^8.0.5",
    "@types/sqlite3": "^3.1.11",
    "@types/three": "^0.174.0",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}
